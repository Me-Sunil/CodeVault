package Stack;

import java.util.Stack;

public class SortedStack {

	public Stack<Integer> sortStack(Stack<Integer> stack) {
		Stack<Integer> rem = new Stack<Integer>();
		while (!stack.empty()) {
			int pope = stack.pop();
			while (!rem.isEmpty() || rem.peek() > pope) {
				stack.push(rem.pop());
			}
			rem.push(pope);
		}
		while (!rem.isEmpty())
			stack.push(rem.pop());
		return stack;
	}

}
